{
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "preserve": "rimraf build && yarn build",
    "serve": "serve -s build",
    "lint": "run-s lint:*",
    "lint:code": "eslint --fix . --ext .js,.jsx,.ts,.tsx",
    "lint:type": "tsc",
    "lint:style": "stylelint --fix \"**/*.{css,scss,sass}\" --aei -q",
    "lint:format": "prettier -w . -u --loglevel silent",
    "commit": "git cz"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix",
    "*.{css,scss,sass}": "stylelint --fix",
    "**/*": "prettier -w -u",
    "package.json": "sort-package-json"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.2",
    "axios": "^0.24.0",
